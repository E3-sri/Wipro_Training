package home;
import java.util.Scanner;

public class MatrixAddition
{

	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		
		int [][] mul_dim_arr1=new int [2][2];
		
		int [][] mul_dim_arr2= new int[2][2];
		
		
		int mul_dim;
		
		for (int i=0;i<(mul_dim_arr1).length;i++)
		{
			for (int j=0;j<(mul_dim_arr2).length;j++)
			{
				 mul_dim_arr1[i][j]=sc.nextInt();
				 
				 mul_dim_arr2[i][j]=sc.nextInt();
				 
			}
		}
		
		
		System.out.println("Printing mul_dim_array1\n");
		
		for (int i=0;i<(mul_dim_arr1).length;i++)
		{
			for (int j=0;j<(mul_dim_arr2).length;j++)
			{
				System.out.println(mul_dim_arr1);
				
			}
		}
			System.out.println("Printing mul_dim_array2\n");
			
			for (int i=0;i<(mul_dim_arr1).length;i++)
			{
				for (int j=0;j<(mul_dim_arr2).length;j++)
				{
					System.out.println(mul_dim_arr2);
					
				}
			
			}
	
	}
}
