package home;
import java.util.Scanner;

public class MatrixAddition
{

	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		
		int [][] mul_dim_arr1=new int [3][3];
		
		int [][] mul_dim_arr2= new int[3][3];
		
		
		int mul_dim;
		
		System.out.println("Enter data in to arrays 3*3:\n");
		for (int i=0;i<(mul_dim_arr1).length;i++)
		{
			for (int j=0;j<(mul_dim_arr2).length;j++)
			{
				 mul_dim_arr1[i][j]=sc.nextInt();
				 
				 mul_dim_arr2[i][j]=sc.nextInt();
				 
			}
		}
		
			System.out.println();

			System.out.println("Printing Matrix1\n");
		
			for (int i=0;i<(mul_dim_arr1).length;i++)
			{
			for (int j=0;j<(mul_dim_arr2).length;j++)
			{
				System.out.print(mul_dim_arr1[i][j]+" ");
				
			}
			System.out.println();
			}
			
			
			System.out.println();
			System.out.println("Printing Matrix2\n");
			
			for (int i=0;i<(mul_dim_arr1).length;i++)
			{
				for (int j=0;j<(mul_dim_arr2).length;j++)
				{
					System.out.print(mul_dim_arr2[i][j]+" ");
					
				}
				System.out.println();
			
			}
			
			System.out.println();
			System.out.println("Addition of matrix\n");
			
			for (int i=0;i<(mul_dim_arr1).length;i++)
			{
				for (int j=0;j<(mul_dim_arr2).length;j++)
				{
					mul_dim= mul_dim_arr1[i][j]+mul_dim_arr2[i][j];
					System.out.print(mul_dim+" ");
					
				}
				System.out.println();
			
			}
			
			System.out.println();
			System.out.println("Subtraction of matrix\n");
			
			for (int i=0;i<(mul_dim_arr1).length;i++)
			{
				for (int j=0;j<(mul_dim_arr2).length;j++)
				{
					mul_dim= mul_dim_arr1[i][j]-mul_dim_arr2[i][j];
					System.out.print(mul_dim+" ");
					
				}
				System.out.println();
			
			}
			
			System.out.println();
			System.out.println("Multiplication of matrix\n");
			
			for (int i=0;i<(mul_dim_arr1).length;i++)
			{
				for (int j=0;j<(mul_dim_arr2).length;j++)
				{
					mul_dim= mul_dim_arr1[i][j]*mul_dim_arr2[i][j];
					System.out.print(mul_dim+" ");
					
				}
				System.out.println();
			
			}
			
			System.out.println();
			System.out.println("Division of matrix\n");
			
			for (int i=0;i<(mul_dim_arr1).length;i++)
			{
				for (int j=0;j<(mul_dim_arr2).length;j++)
				{
					mul_dim= mul_dim_arr1[i][j]/mul_dim_arr2[i][j];
					System.out.print(mul_dim+" ");
					
				}
				System.out.println();
			
			}

	}
	
}

